{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
    "parser": {
        "syntax": "typescript",
        "dynamicImport": true,
        "decorators": true
    },
    "target": "esnext",
    "loose": true,
    "externalHelpers": true,
    "keepClassNames": true
    },
    "minify": false,
    "module": {
        "type": "es6",
        "strict": false,
        "strictMode": false,
        "lazy": false,
        "noInterop": false
    }
}
   