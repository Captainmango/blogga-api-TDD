{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "dynamicImport": true,
            "decorators": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "target": "es2020",
        "loose": true,
        "externalHelpers": true,
        "keepClassNames": true,
        "baseUrl": "./app"
    },
    "minify": false,
    "module": {
        "type": "es6",
        "strict": false,
        "strictMode": false,
        "lazy": false
    }
}
   