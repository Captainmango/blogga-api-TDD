{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
    "parser": {
        "syntax": "typescript",
        "dynamicImport": false,
        "decorators": true
    },
    "target": "esnext",
    "loose": false,
    "externalHelpers": false,
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": false
    },
    "minify": false,
    "module": {
        "type": "es6",
        "strict": false,
        "strictMode": false,
        "lazy": false,
        "noInterop": false
    },
    "swcrc": true
}
   